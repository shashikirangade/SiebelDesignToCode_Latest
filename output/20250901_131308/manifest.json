{
  "version": "1.0",
  "containers": [
    {
      "name": "Sidebar",
      "role": "complementary",
      "selector": ".sidebar",
      "type": "container",
      "children": [
        {
          "name": "SidebarHeader",
          "role": "content",
          "selector": ".sidebar__header",
          "type": "applet"
        },
        {
          "name": "ContactsSection",
          "role": "list",
          "selector": ".sidebar__section",
          "type": "applet",
          "item_selector": ".sidebar__list-item"
        },
        {
          "name": "ServiceRequestsSection",
          "role": "list",
          "selector": ".sidebar__section",
          "type": "applet",
          "item_selector": ".sidebar__list-item"
        },
        {
          "name": "OpportunitiesSection",
          "role": "list",
          "selector": ".sidebar__section",
          "type": "applet",
          "item_selector": ".sidebar__list-item"
        }
      ]
    },
    {
      "name": "MainContent",
      "role": "main",
      "selector": ".main-content",
      "type": "container",
      "children": [
        {
          "name": "HeaderButtons",
          "role": "navigation",
          "selector": ".main-content__header-buttons",
          "type": "applet"
        },
        {
          "name": "ContentItems",
          "role": "list",
          "selector": ".main-content",
          "type": "applet",
          "item_selector": ".main-content__list-item"
        }
      ]
    }
  ]
}