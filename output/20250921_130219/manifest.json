{
  "version": "1.0",
  "containers": [
    {
      "name": "Sidebar",
      "role": "complementary",
      "selector": ".sidebar",
      "type": "container",
      "children": [
        {
          "name": "Header",
          "role": "banner",
          "selector": ".header",
          "type": "applet"
        },
        {
          "name": "ContactsSection",
          "role": "list",
          "selector": ".contacts",
          "type": "applet",
          "item_selector": ".contacts__item"
        },
        {
          "name": "ServiceRequestsSection",
          "role": "list",
          "selector": ".service-requests",
          "type": "applet",
          "item_selector": ".service-requests__item"
        },
        {
          "name": "OpportunitiesSection",
          "role": "list",
          "selector": ".opportunities",
          "type": "applet",
          "item_selector": ".opportunities__item"
        }
      ]
    },
    {
      "name": "MainContent",
      "role": "main",
      "selector": ".main-content",
      "type": "container",
      "children": [
        {
          "name": "Timeline",
          "role": "list",
          "selector": ".timeline",
          "type": "applet",
          "item_selector": ".timeline__item"
        }
      ]
    }
  ]
}